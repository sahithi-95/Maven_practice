pipeline{
     agent any
     
     stages{
      stage ("Check stage"){
      steps{
          withMaven(maven : 'mvn'){
          sh 'mvn -- version'
      }
      
      }
      
      stage ("Compile stage'){
      steps{
           withMaven(maven : 'mvn'){
      sh 'mvn -f mvn mvn-practice/pom.xml clean compile'
      }
      
      }
       stage ('Test stage'){
       steps{
             sh 'mvn -f mvn mvn-practice/pom.xml test'
       sh 'mvn test'
       }
       
       }
       }
       
       
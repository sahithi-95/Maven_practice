pipeline{
     agent any
     
     stages {
      stage ('Check stage'){
          steps {
              withMaven(maven : 'mvn'){
                  bat 'mvn -- version'
                }
        
              }
      }
      
      stage ('Compile stage'){
      steps{
           withMaven(maven : 'mvn') {
          bat 'mvn -f mvn-practice/pom.xml compile'
          }
        }
      
      }
       stage ('Test stage'){
         steps{
            withMaven(maven: 'mvn'){
             bat 'mvn -f mvn-practice/pom.xml install'
       
             }
           }
       
         }
       }
     }
       
       
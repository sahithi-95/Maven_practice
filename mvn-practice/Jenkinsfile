pipeline{

agent any

stages{
stage('Check Stage'){
steps{
withMaven(maven : 'mvn'){
bat 'mvn --version'
}
}
}
stage('Compile Stage'){
steps{
withMaven(maven : 'mvn'){
bat 'mvn -f mvn-math/pom.xml compile'
}
}
}
stage('Test Stage'){
steps{
withMaven(maven : 'mvn'){
bat 'mvn -f mvn-math/pom.xml install'
}
}
}
}
}